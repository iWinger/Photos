<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginController">
   <children>
      <AnchorPane layoutX="-6.0" layoutY="-14.0" prefHeight="200.0" prefWidth="200.0">
         <children>
            
            <TextField fx:id="User" focusTraversable="false" layoutX="24.0" layoutY="74.0" promptText="Username: " />
            <Button fx:id="Exit" layoutX="119.0" layoutY="137.0" mnemonicParsing="false" onAction="#handleExitButtonAction" text="Exit" />
            
            <Button fx:id="Login" layoutX="24.0" layoutY="137.0" mnemonicParsing="false" onAction="#handleLoginButtonAction" text="Login" />
            
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
